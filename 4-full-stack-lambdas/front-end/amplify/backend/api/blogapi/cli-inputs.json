{
  "version": 1,
  "paths": {
    "/api": {
      "name": "/api",
      "lambdaFunction": "blogapi",
      "permissions": {
        "setting": "open"
      }
    },
    "/api/post": {
      "name": "/api/post",
      "lambdaFunction": "blogapi",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "update",
          "delete"
        ]
      }
    },
    "/api/post/get": {
      "name": "/api/post/get",
      "lambdaFunction": "blogapi",
      "permissions": {
        "setting": "open"
      }
    }
  }
}